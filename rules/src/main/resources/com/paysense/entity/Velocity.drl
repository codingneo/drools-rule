package com.paysense.entity;

rule "Velocity Reject"
    when
        TranObjectContainer(newTransaction.status == 0, $tran:newTransaction, $oldTrans:oldTransactions)
    then
        System.out.println( "inside Velocity Eligible rule - old transactions size: "+$oldTrans.size());
  
  	if($oldTrans.size()>2) {
        	$tran.setStatus(3);      
        } 
end